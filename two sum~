class Solution {
public:
    vector<int> twoSum(vector<int> &numbers, int target) {
        for (int i=0;i<numbers.size();i++){
            for (int j=0;j<numbers.size();i++){
                if (i==j){
                    continue;
                }
                if(i+j==target){
                    break;
                }
            }
            if(i+j==target){
                break;
            }
        }
        vector<int> ret(i,j);
        return ret;
    }
};
